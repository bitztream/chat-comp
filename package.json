{
  "name": "chat-comp",
  "version": "1.0.0",
  "description": "Chat component for bitztream",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch --coverage --passWithNoTests",
    "seed": "node seed.js",
    "build": "webpack --open",
    "server": "node server.js",
    "start": "webpack-dev-server --open --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitztream/chat-comp.git"
  },
  "author": "Austin Pittman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bitztream/chat-comp/issues"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "<rootDir>/__tests__/setup/handleCss.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>__tests__/setup/setupEnzyme.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ]
  },
  "homepage": "https://github.com/bitztream/chat-comp#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^25.1.0",
    "mongoose": "^5.9.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "simplebar-react": "^2.1.0",
    "styled-components": "^5.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "ascii": "^1.0.0",
    "ascii-art-image": "^1.2.2",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "html-loader": "^4.2.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.3.0",
    "webpack-dev-server": "^3.10.3"
